{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "Db": {
    "ConnectionString": "Host=testpostgres;Database=currency_db_test;Username=postgres;Password=admin"
  },
  "CurrencyApi": {
    "ApiUrl": "https://www.cbr.ru/scripts/XML_daily.asp",
    "Retry": {
      "Retries": 3,
      "DelayInSeconds": 10
    }
  },
  "BackgroundService": {
    "DelayInMinutes": 10
  },
  "AllowedHosts": "*"
}
